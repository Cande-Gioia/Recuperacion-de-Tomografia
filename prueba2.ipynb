   "source": [
    "import numpy as np\n",
    "pi = np.pi\n",
    "cos = np.cos\n",
    "sin = np.sin\n",
    "\n",
    "ang = 90\n",
    "rot = np.array([[cos(ang*pi/180), -sin(ang*pi/180)], \n",
    "                [sin(ang*pi/180), cos(ang*pi/180)]], dtype=np.float32)\n",
    "\n",
    "v = [1, 0]\n",
    "print(rot @ v)"
   ]
